{
    "manifest_version": 3,
    "name": "CookieOnZeSite",
    "version": "1.0",
    "permissions": ["cookies", "activeTab", "scripting", "storage"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  